<div id="dashWrapper">

    <div id="#Dashboard" style="height:100%;">

        @Html.DevExpress().Dashboard(settings =>
    {
        settings.Name = Model.DashboardId;
        settings.Width = Unit.Percentage(100);
        settings.Height = Unit.Percentage(100);
        settings.EnableSqlDataSource = true;
        settings.WorkingMode = DevExpress.DashboardWeb.WorkingMode.ViewerOnly;
        settings.InitialDashboardId = Model.DashboardId;
    }).GetHtml()
    </div>

    </div>
